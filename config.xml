<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.example"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>AMTG iPad App</name>

    <description>
       AMTG iPad App
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Hardeep Shoker 
    </author>
    <preference name="phonegap-version" value="2.9.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="tablet" />
    <preference name="fullscreen" value="true" />
    <icon src="icons/amtg-ipad-icon.png" />
    <gap:splash src="icons/Default-Portrait~ipad.png" gap:platform="ios" width="320" height="480" />
    <gap:splash src="icons/Default-Portrait@2x~ipad.png" gap:platform="ios" width="768" height="1004" />


</widget>