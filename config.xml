<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.example"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>AMTG iPad App</name>

    <description>
       AMTG iPad App
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Hardeep Shoker 
    </author>
    <preference name="phonegap-version" value="2.9.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="tablet" />
    <preference name="fullscreen" value="false" />
        
    <icon src="icon/72x72.png" gap:platform="ios" width="72" height="72" />
<!-- retina iPad support: PhoneGap 2.5.0+ only -->
    <icon src="icon/512x512.png" gap:platform="ios" width="144" height="144" />

    <plugin name="InAppBrowser" value="CDVInAppBrowser" />
    
    <!-- <gap:splash src="icon/w1004.png" gap:platform="ios" width="1024" height="748" /> -->
    <gap:splash src="icon/h1004.png" gap:platform="ios" width="768" height="1004" />

    <!-- retina iPad support: PhoneGap 2.5.0+ only -->
    <gap:splash src="icon/w2008.png" gap:platform="ios" width="2048" height="1496" />
    <gap:splash src="icon/h2008.png" gap:platform="ios" width="1536" height="2008" />

    <gap:platform name="ios" />

</widget>